<p id="notice"><%= notice %></p>

<h1>User Pagecomments</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Pagecomment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_pagecomments.each do |user_pagecomment| %>
      <tr>
        <td><%= user_pagecomment.user_id %></td>
        <td><%= user_pagecomment.pagecomment_id %></td>
        <td><%= link_to 'Show', user_pagecomment %></td>
        <td><%= link_to 'Edit', edit_user_pagecomment_path(user_pagecomment) %></td>
        <td><%= link_to 'Destroy', user_pagecomment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Pagecomment', new_user_pagecomment_path %>
